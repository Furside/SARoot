@model MobilePlan.Models.NetworkType
@{ 
    
}
<div class="col-md-12">
	@if(ViewBag.Message != null)
	{
		<p class="alert alert-success">@ViewBag.Message</p>
	}
	@Html.AntiForgeryToken()
	@Html.ValidationSummary(true, "", new { @class = "text-danger" })
</div>
<div class="col-md-12">
@Html.LabelFor(model => model.Network)
@Html.EditorFor(model => model.Network, new { htmlAttributes = new { @class = "form-control form-control-sm" } })
@Html.ValidationMessageFor(model => model.Network, "", new { @class = "text-danger" })
</div>

<script>
    @ViewBag.Execute
</script>